datasource db {
  provider = "postgresql"
  url      = env("DATABASE-URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id          Int    @id @default(autoincrement())
  name        String @unique
  description String
  score       Int

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
